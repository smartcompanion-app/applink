{
    "name": "smartcompanion/applink",
    "type": "project",
    "description": "One shareable link to all your app market listings",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "symfony/yaml": "^7.1"        
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "phpstan/phpstan": "^2.0",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "SmartCompanion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartCompanion\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
